import pydotplus
import pandas as pd
import numpy as np
data1=pd.read_csv("Foreign tourist inflow 2001-14.csv")
data2=data1[data1.columns[4:15]]data2.head()
#has to be imported if not already installed in condafrom sklearn.tree
import DecisionTreeClassifier#for exporting to a pdffrom sklearn.tree 
import export_graphvizfrom sklearn.model_selection
import train_test_split x_train,x_test,y_train,y_test=train_test_split(data2.iloc[1:5],data2.iloc[6:10],random_state=1)
clf=DecisionTreeClassifier()clf.fit(x_train,y_train)
#This is not saved to any external file it is supposed to return data accordingly
#Gives a file in .dat format that is exported to pdf accordingly
dot_data=export_graphviz(clf,out_file=None)
graph=pydotplus.graph_from_dot_data(dot_data)
#The graph saved to a file named iris.pdfgraph.write_pdf("final2.pdf")#returns a graph object
