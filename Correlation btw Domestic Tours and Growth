import pandas as pd
import numpy as np
import matplotlib.pyplot as plt
data=pd.read_csv("State tourist inflow 2013-14.csv")
data.head()
data.tail()
data.fillna(0)
x=data["2013 - DTVs"].reshape(-1,1)
y=data["Growth 2014/13 - DTVs"].reshape(-1,1)
from sklearn import model_selectionX_train,X_test,Y_train,Y_test=model_selection.train_test_split(x,y)
from sklearn.linear_model import LinearRegression
alg=LinearRegression()
alg.fit(X_train,Y_train)
import matplotlib.pyplot as plt
m=alg.coef_[0]
c=alg.intercept_
x_line=np.arange(0,10,0.1)
y_line=m*x_line+c
plt.plot(x_line,y_line)
plt.show()    
